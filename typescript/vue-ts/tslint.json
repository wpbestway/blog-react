{
  "defaultSeverity": "error",
  "extends": [
      "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
      "quotemark": [
          false,
          "jsx-single"
      ],
      "semicolon": false,
      "no-unnecessary-initializer": false,
      "no-console": false,
      "ordered-imports": false,
      "object-literal-sort-keys": false,
      "variable-name": false,
      "no-var-requires": false
  },
  "rulesDirectory": []
}